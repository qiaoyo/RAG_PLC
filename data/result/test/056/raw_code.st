VAR_INPUT
    STR : STRING;
END_VAR

VAR_OUTPUT
    STR_TO_DW : DWORD;
END_VAR

VAR
    TEMP : STRING[10];
    I : INT;
    CHAR_VAL : BYTE;
    RESULT : DWORD;
END_VAR

TEMP := STR;
RESULT := DWORD#0;

FOR I := 1 TO LEN(TEMP) DO
    CHAR_VAL := BYTE(TEMP[I]);
    IF CHAR_VAL >= BYTE('0') AND CHAR_VAL <= BYTE('9') THEN
        RESULT := RESULT * DWORD#10 + DWORD(CHAR_VAL - BYTE('0'));
    ELSIF CHAR_VAL >= BYTE('A') AND CHAR_VAL <= BYTE('F') THEN
        RESULT := RESULT * DWORD#16 + DWORD(CHAR_VAL - BYTE('A') + 10);
    ELSIF CHAR_VAL >= BYTE('a') AND CHAR_VAL <= BYTE('f') THEN
        RESULT := RESULT * DWORD#16 + DWORD(CHAR_VAL - BYTE('a') + 10);
    END_IF;
END_FOR;

STR_TO_DW := RESULT;